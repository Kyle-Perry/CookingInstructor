  <Recipe>
    <recipeName>Pasta 1</recipeName>
    <category>Main</category>
    <photoPath>pasta.jpg</photoPath>
    <ingredients>
      <Ingredient>
        <name>Tomato Sauce</name>
        <measure>ml</measure>
        <amount>1000</amount>
      </Ingredient>
      <Ingredient>
        <name>Pasta</name>
        <measure>g</measure>
        <amount>200</amount>
      </Ingredient>
    </ingredients>
    <instructions>
      <Instruction>
        <info>Boil tomato sauce.</info>
        <photoPath>boilSauce.jpg</photoPath>
      </Instruction>
      <Instruction>
        <info>Boil pasta after water has been boiled.</info>
        <photoPath>boilPasta.jpg</photoPath>
      </Instruction>
      <Instruction>
        <info>Put pasta on plate and pour sauce over it. Serve Hot.</info>
        <photoPath>pasta.jpg</photoPath>
      </Instruction>
    </instructions>
    <isFavorite>true</isFavorite>
  </Recipe>
